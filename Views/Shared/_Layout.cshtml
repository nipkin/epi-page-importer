@model EpiPageImporter.Models.ViewModels.IPageViewModel<EpiPageImporter.Models.Pages.BasePageData>
@using EPiServer.Web.Mvc.Html

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        @Url.ContentUrl(Model.Layout.StartPageLink)
        @Model.Layout.MainMenu
    </header>
    <div>
        @RenderBody()
    </div>
</body>
</html>